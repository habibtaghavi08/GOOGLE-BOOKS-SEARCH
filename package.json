{
  "name": "GOOGLE-BOOKS-SEARCH",
  "version": "1.0.0",
  "description": "HW ASSIGNMENT USING REACT AND ALSO A MERN STACK",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habibtaghavi08/GOOGLE-BOOKS-SEARCH.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/habibtaghavi08/GOOGLE-BOOKS-SEARCH/issues"
  },
  "homepage": "https://github.com/habibtaghavi08/GOOGLE-BOOKS-SEARCH#readme",
  "dependencies": {
    "mongoose": "^5.9.16"
  }
}
